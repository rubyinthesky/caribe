<p id="notice"><%= notice %></p>

<h1>Hashts</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @hashts.each do |hasht| %>
      <tr>
        <td><%= hasht.nombre %></td>
        <td><%= link_to 'Show', hasht %></td>
        <td><%= link_to 'Edit', edit_hasht_path(hasht) %></td>
        <td><%= link_to 'Destroy', hasht, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Hasht', new_hasht_path %>
